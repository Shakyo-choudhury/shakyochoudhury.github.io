{
    "builds": [
      {
        "src": "codewithshakyo/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdasize":"15mb", "runtime":"python 3.8"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "codewithshakyo/wsgi.py"
      }
    ]
  }

{ "rewrites": [{ "source": "/(.*)", "destination": "/" }] }
